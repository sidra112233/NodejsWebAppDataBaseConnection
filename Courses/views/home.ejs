              c.classList.remove('open');
              c.querySelector('span').innerHTML = '<i class="fas fa-plus"></i>';
          });

          if (!isOpen) {
              card.classList.add('open');
              card.querySelector('span').innerHTML = '<i class="fas fa-minus"></i>';
              card.scrollIntoView({ behavior: 'smooth', block: 'center' });
          }
      }
  </script>
