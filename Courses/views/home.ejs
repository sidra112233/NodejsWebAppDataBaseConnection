        /* Slideshow container */
        .s {
            margin-top: 0px;
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100vh;
            background: linear-gradient(135deg, #1f1f1f, #acaaaa); /* Dark gradient background */
            font-family: Arial, sans-serif;
            overflow: hidden;
        }

        .scene {
            position: relative;
            width: 200vw; /* Width relative to viewport width */
            height: 95vh; /* Height relative to viewport height */
            max-width: 1150px; /* Maximum width for large screens */
            max-height: 800px; /* Maximum height for large screens */
            background-color: #c9c4c4; /* Dark gray background for the scene */
            border-radius: 15px;
            overflow: hidden;
            box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.3);
            text-align: center;
        }

        .girl {
            position: absolute;
            left: 50%;
            bottom: 0;
            transform: translateX(-50%) translateY(100px);
            opacity: 0;
            transition: opacity 1s ease-in-out, transform 1s ease-in-out;
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        .girl.show {
            opacity: 1;
            transform: translateX(-50%) translateY(0);
        }

        .girl video {
            width: 1150px;
            height: auto;
            border-radius: 10px;
        }

        .speech-bubble {
            position: absolute;
            left: 50%;
            bottom: 5px;
            transform: translateX(-50%) scale(0);
            background-color: #e0f7fa; /* Light cyan background for a professional look */
            border: 2px solid #00838f; /* Darker border color */
            padding: 15px 25px;
            border-radius: 20px;
            font-size: 18px;
            opacity: 0;
            transition: opacity 1s ease-in-out, transform 1s ease-in-out;
        }

        .speech-bubble.show {
            opacity: 1;
            transform: translateX(-50%) scale(1);
        }

        .balloons {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            pointer-events: none; /* Ensure balloons don't block interactions */
            opacity: 0;
            transition: opacity 1s ease-in-out;
        }

        .balloon {
            width: 100px;
            height: 140px;
            border-radius: 50% 50% 50% 50% / 60% 60% 40% 40%;
            position: absolute;
            font-size: 18px;
            font-weight: bold;
            color: #fff;
            display: flex;
            justify-content: center;
            align-items: center;
            opacity: 0;
            transition: opacity 1s ease-in-out, transform 1s ease-in-out;
            z-index: 10;
        }

        .balloon.blue {
            background-color: #00aaff; /* Light blue */
        }

        .balloon.green {
            background-color: #66bb6a; /* Light green */
        }

        .balloon.purple {
            background-color: #ab47bc; /* Light purple */
        }

        .balloon.orange {
            background-color: #ff7043; /* Light orange */
        }

        .balloon.show {
            opacity: 1;
            animation: float 12s linear infinite;
        }

        /* Adjusted animation for smoother flow */
        @keyframes float {
            0% { transform: translateY(60vh) translateX(0); }
            100% { transform: translateY(-60vh) translateX(50px); }
        }

        .picture {
            display: none; /* Initially hidden */
            position: absolute;
            left: 50%;
            top: 50%;
            transform: translate(-50%, -50%);
            width: 1150px; /* Adjust width as needed */
            height: auto;
            border-radius: 15px;
            box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.5);
        }

        .picture.show {
            display: block;  
        }

        .thank-you-message {
            display: none; /* Initially hidden */
            position: absolute;
            left: 50%;
            top: 95%; /* Adjusted to be just above the bottom */
            transform: translate(-50%, -50%);
            font-size: 24px;
            color: #ffffff;
            background-color: #00838f; /* Dark cyan background */
            padding: 10px 20px;
            border-radius: 10px;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.5);
        }

        .thank-you-message.show {
            display: block; /* Show the message along with the picture */
        }
