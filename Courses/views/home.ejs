      <script>
          function startAnimation() {
              const girl = document.querySelector('.girl');
              const speechBubble = document.querySelector('.speech-bubble');
              const balloons = document.querySelector('.balloons');
              const picture = document.querySelector('.picture');
              const thankYouMessage = document.querySelector('.thank-you-message');
  
              // Show the girl after 1 second
              setTimeout(() => {
                  girl.classList.add('show');
              }, 1000);
  
              // Show the speech bubble and use Web Speech API to speak the text
              setTimeout(() => {
                  speechBubble.classList.add('show');
  
                  // Use Web Speech API to speak the welcome message
                  const message = new SpeechSynthesisUtterance("Welcome to Learn Code Pro! Learn from basic to advanced levels.");
                  message.onend = function() {
                      // After the speech ends, hide the speech bubble and girl, then show balloons
                      speechBubble.classList.remove('show');
                      girl.classList.remove('show');
                      balloons.style.opacity = 1; // Show balloons container
                      const balloonElements = document.querySelectorAll('.balloon');
                      balloonElements.forEach((balloon, index) => {
                          setTimeout(() => {
                              balloon.classList.add('show');
                          }, index * 300); // Staggered balloon appearance
                      });
  
                      // Hide the balloons and show the picture and thank you message together after balloons are displayed
                      setTimeout(() => {
                          balloons.style.opacity = 0; // Hide balloons
                          picture.classList.add('show');
                          thankYouMessage.classList.add('show');
                          
                          // Use Web Speech API to speak the thank you message
                          const thankYouSpeech = new SpeechSynthesisUtterance("Thanks for coming to our website!");
                          window.speechSynthesis.speak(thankYouSpeech);
                          
                          // Reset elements for next iteration
                          setTimeout(() => {
                              picture.classList.remove('show');
                              thankYouMessage.classList.remove('show');
                              startAnimation(); // Repeat animation
                          }, 10000); // Delay before repeating
                      }, balloonElements.length * 300 + 5000); // Delay showing the picture and message until all balloons are displayed
                  };
                  window.speechSynthesis.speak(message);
              }, 3000); // Delay before speech bubble shows
          }
  
          // Start the animation loop
          startAnimation();
      </script>
